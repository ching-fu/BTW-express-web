<script>
    $(document).ready(function () {

        $('.tab li').click(function (e) {

            var index = $(e.currentTarget).index();
            $('.tab li').removeClass('active').eq(index).addClass('active');
            $('.content').removeClass('active').eq(index).addClass('active');

        });

    });
</script>
